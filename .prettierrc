{
	"trailingComma": "es5",
	"tabWidth": 2,
	"semi": true,
	"singleQuote": true,
	"printWidth": 200,
	"useTabs": false,
	"bracketSpacing": true,
	"quoteProps": "preserve",
	"endOfLine": "lf",
	"arrowParens": "always",
	"overrides": [
		{
			"files": "src/**/*.repository.ts",
			"options": {
				"printWidth": 100
			}
		},
		{
			"files": "src/app.module.ts",
			"options": {
				"printWidth": 100
			}
		}
	]
}
  